ntData = await getRentedBooks(1);\r\n      setRentData(rentData);\r\n    }\r\n    rentBook()\r\n    fetchBooks();\r\n  }, []);\r\n\r\n    \r\n  useEffect(() => {\r\n    const fetchCategories = async () => {\r\n      const categoryData = await getAllCategories();\r\n      setCategories(categoryData)\r\n    }\r\n    const isAuthenticated = ()=>{\r\n      if (currentUser == null){\r\n        history.push('/login')\r\n      }\r\n    }\r\n    isAuthenticated();\r\n    fetchCategories();\r\n  },[]);\r\n  \r\n  \r\n  const handleCreate = async (bookData) => {\r\n    const newBook = await postBook(bookData);\r\n    setBooks(prevState => [...prevState, newBook])\r\n    history.push('/books')\r\n  }\r\n  const handleDelete = async (id) => {\r\n    await deleteBook(id);\r\n    setBooks(prevState => prevState.filter(book => {\r\n      return book.id !== id\r\n    }))\r\n    history.push('/books')\r\n  }\r\n  const handleUpdate = async (id, bookData) => {\r\n    const updatedBook = await putBook(id, bookData);\r\n    setBooks(prevState => prevState.map(book => {\r\n      return book.id === Number(id) ? updatedBook : book\r\n    }))\r\n    history.push('/books')\r\n  }\r\n\r\n  const handleFilters = async(filterData) =>{\r\n    const books = await filterBook(filterData);\r\n    setBooks(books)\r\n  }\r\n  const handleRentCreate = async(id, rentData) => {\r\n    const books = await rentCreate(id, rentData);\r\n    setRentData(books);\r\n    history.push('/profile')\r\n  }\r\n\r\n  \r\n  const handleRentBookDelete = async(id)=>{\r\n    const data = await rentBookDelete(id);\r\n    setRentData(data);\r\n  }\r\n  const handleBookRating = async(id, rating)=>{\r\n    const data = await updateRating(id, rating);\r\n    setRentData(data);\r\n  }\r\n  \r\n\r\n  return (\r\n    <Switch>\r\n        <Route exact path='/books'>\r\n        <Catalog\r\n          books={books}\r\n          handleDelete={handleDelete}\r\n          currentUser={currentUser}\r\n          handleFilters = {handleFilters}\r\n        />\r\n      </Route>\r\n      <Route path='/sellYourBook'>\r\n        <SellYourBook\r\n          handleCreate={handleCreate}\r\n          categories={categories}\r\n        />\r\n      </Route>\r\n      <Route exact path='/books/:id'>\r\n        <BookInfo\r\n          books={books}\r\n          handleDelete={handleDelete}\r\n        />\r\n      </Route>\r\n      <Route path='/books/:id/edit'>\r\n        <Customize\r\n          books={books}\r\n          handleUpdate={handleUpdate}\r\n          categories={categories}\r\n        />\r\n      </Route>\r\n      <Route path='/sellyourbook'>\r\n        <SellYourBook\r\n          handleCreate={handleCreate}\r\n        />\r\n      </Route>\r\n      <Route path='/books/:id/rents'>\r\n        <ShoppingCard\r\n         handleRentCreate={handleRentCreate}\r\n        />\r\n      </Route>\r\n      <Route exact path='/'>\r\n        <Landing\r\n          \r\n        />\r\n      </Route>\r\n       <Route exact path='/profile'>\r\n        <Profile\r\n        handleRentBookDelete={handleRentBookDelete}  \r\n        rentData = {rentData}\r\n        handleBookRating = {handleBookRating}      \r\n        />\r\n        \r\n      </Route>\r\n    </Switch>\r\n  )\r\n}\r\n","C:\\Users\\ansar\\OneDrive\\Desktop\\BookLand2-main\\client\\src\\screens\\SellYourBook.jsx",[],"C:\\Users\\ansar\\OneDrive\\Desktop\\BookLand2-main\\client\\src\\index.js",[],["69","70"],"C:\\Users\\ansar\\OneDrive\\Desktop\\BookLand2-main\\client\\src\\screens\\Landing.jsx",[],"C:\\Users\\ansar\\OneDrive\\Desktop\\BookLand2-main\\client\\src\\layouts\\Layout.jsx",[],"C:\\Users\\ansar\\OneDrive\\Desktop\\BookLand2-main\\client\\src\\screens\\Register.jsx",[],"C:\\Users\\ansar\\OneDrive\\Desktop\\BookLand2-main\\client\\src\\screens\\BookInfo.jsx",[],"C:\\Users\\ansar\\OneDrive\\Desktop\\BookLand2-main\\client\\src\\screens\\ShoppingCard.jsx",[],"C:\\Users\\ansar\\OneDrive\\Desktop\\BookLand2-main\\client\\src\\services\\books.js",[],"C:\\Users\\ansar\\On